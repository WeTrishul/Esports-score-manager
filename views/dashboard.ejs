        <script>
             class ToggleLike{
        constructor(toggleElement){
            this.toggler = toggleElement;
            this.toggleLike();
        }
        toggleLike(){
            $(this.toggler).click(function(e){
                e.preventDefault();
                let self = this;
               
                console.log($(self).attr('href'))

                var jsonstring = $(self).attr('value');
               
            

                $.ajax({
                type: 'POST',
                data:jsonstring,
                url: $(self).attr('href'),
            })
            .done(function(data) {
                $('#event-'+jsonstring).remove()
            })
            .fail(function(errData) {
                console.log('Error aaya');
            });

            })
        }
    }
        </script>
        <script>
            $('.anevent').each(function(){
                let self = this;
               
                let toggleLike = new ToggleLike(self);
            });
        </script>

    
